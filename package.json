{
  "name": "@jetbrains/babel-preset-jetbrains",
  "version": "2.0.1",
  "description": "JetBrains preset for Babel",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:JetBrains/babel-preset.git"
  },
  "keywords": [
    "babel",
    "babel-preset"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-transform-runtime": "7.0.0",
    "@babel/plugin-transform-strict-mode": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-angularjs-annotate": "0.9.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@jetbrains/eslint-config": "^4.0.2",
    "ci-publish": "^1.3.1",
    "commitizen": "^2.10.1",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^5.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "standard-version": "^4.4.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "scripts": {
    "precommit": "lint-staged",
    "commit": "git-cz",
    "lint": "eslint .",
    "release:ci": "standard-version && git push --follow-tags origin master && ci-publish"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">8.0.0"
  }
}
